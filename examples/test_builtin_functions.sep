# SEP DSL Test Script for Built-in Functions
# This tests the core primitives, math, and statistical functions

# Test type checking functions
x = 42.5
str_val = "hello"
bool_val = true

print("Testing type checking functions:")
print("is_number(42.5):", is_number(x))
print("is_string('hello'):", is_string(str_val))
print("is_bool(true):", is_bool(bool_val))

# Test conversion functions
print("\nTesting conversion functions:")
print("to_string(42.5):", to_string(x))
print("to_number('123.45'):", to_number("123.45"))

# Test math functions
print("\nTesting math functions:")
print("abs(-5.5):", abs(-5.5))
print("min(10, 20):", min(10, 20))
print("max(10, 20):", max(10, 20))
print("sqrt(16):", sqrt(16))
print("pow(2, 3):", pow(2, 3))

# Test trigonometric functions
print("\nTesting trigonometric functions:")
print("sin(1.5708):", sin(1.5708))  # ~pi/2
print("cos(0):", cos(0))
print("tan(0.7854):", tan(0.7854))  # ~pi/4

# Test statistical functions
print("\nTesting statistical functions:")
print("mean(1, 2, 3, 4, 5):", mean(1, 2, 3, 4, 5))
print("median(1, 2, 3, 4, 5):", median(1, 2, 3, 4, 5))
print("std_dev(1, 2, 3, 4, 5):", std_dev(1, 2, 3, 4, 5))

print("\nAll built-in functions tested successfully!")
