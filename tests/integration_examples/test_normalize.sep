// Test normalize function
pattern test_normalize {
    // Test normalization  
    v = vec3(5, 0, 0)
    v_len = length(v)
    print("Before normalize: length =", v_len)
    
    normalized = normalize(v)
    print("Normalized vector =", normalized)
    
    new_len = length(normalized)
    print("After normalize: length =", new_len)
}
